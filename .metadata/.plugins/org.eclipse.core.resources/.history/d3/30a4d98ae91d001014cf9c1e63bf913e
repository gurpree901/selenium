package day2;

public class staticdemo {
	static int c=30; //static variable
	int d=40;
	//static method
	static void m1()
	{
		System.out.println("this is static method..");
	}
	
	void m2()
	{
		System.out.println("this is non static method");
	}
	


	public static void main(String[] args) {
		
		System.out.println(c);
		m1();

	}

}
